(defhelper min_max (&optional (fnums *))
  (rewrite* ("min_le" "min_ge" "min_lt" "min_gt"
	     "le_min" "ge_min" "lt_min" "gt_min"
	     "max_le" "max_ge" "max_lt" "max_gt"
	     "le_max" "ge_max" "lt_max" "gt_max") :target-fnums fnums)
  "Rewrites with max/min lemmas in FNUMS"
  "Rewriting with max/min lemmas")
